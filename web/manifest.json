{
    "name": "cybershield",
    "short_name": "cybershield",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "permissions": [
          "activeTab"
      ],
    "content_security_policy": {
      "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/vpn16.png",
        "32": "icons/vpn32.png",
        "64": "icons/vpn64.png",
        "128": "icons/vpn128.png"
      }
    },
    "icons": {
        "16": "icons/vpn16.png",
        "32": "icons/vpn32.png",
        "64": "icons/vpn64.png",
        "128": "icons/vpn128.png"
    }
  }
